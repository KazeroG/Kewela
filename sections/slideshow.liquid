{% if section.blocks.size > 0 %}
<div class="holder {% if section.settings.block_width == 'fullboxed' or section.settings.block_width == 'fullwidth' %}fullwidth{% else %}boxed{% endif %} {{ section.settings.block_width }} {{ section.settings.outer_margin }} full-nopad">
    <div class="container">
    <div class="bnslider-wrapper">
    <div class="bnslider {% if section.settings.keep_scale %}keep-scale{% else %}bnslider--{{ section.settings.slideshow_height }}{% endif %}" {% if section.settings.keep_scale_width %}data-start-width='{{ section.settings.keep_scale_width }}'{% endif %}  {% if section.settings.keep_scale_height %}data-start-height='{{ section.settings.keep_scale_height }}'{% endif %} {% if section.settings.keep_scale_mwidth %}data-start-mwidth='{{ section.settings.keep_scale_mwidth }}'{% endif %}  {% if section.settings.keep_scale_mheight %}data-start-mheight='{{ section.settings.keep_scale_mheight }}'{% endif %} id="bnslider-{{ section.id }}" data-autoplay="{{ section.settings.autoplay }}" data-speed="{{ section.settings.autoplay_speed }}" {% if section.settings.bg_color != blank and section.settings.bg_color != 'rgba(0,0,0,0)' %}style="background-color: {{ section.settings.bg_color }}"{% endif %}>
        {% for block in section.blocks %}
            {% if block.settings.enable %}
                {% unless block.settings.link_all_slide %}
            <div {{ block.shopify_attributes }} class="bnslider-slide" data-autoplay="{% if block.settings.video_autoplay %}true{% else %}false{% endif %}" data-video-type="video">
            {% else %}
                <a {{ block.shopify_attributes }} href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="bnslider-slide {% if block.settings.type == 'background-box' or  block.settings.type == 'background' %}bnslider-slide--background-video{% endif %}">
                    {% endunless %}
                    {% if block.settings.video_enable %}
                        <div class="video-wrap">
                            <video playsinline {% if block.settings.video_loop %}loop{% endif %} {% if block.settings.video_muted %}muted{% endif %} preload="auto" poster="{% if block.settings.image %}{{ block.settings.image | img_url: '2048x' }}{% endif %}">
                                <source src="{{ block.settings.video_mp4 }}" type="video/mp4">
                            </video>
                            {% if block.settings.video_control %}
                                <div class="video-control visible">
                                    <div class="video-play js-video-slider-play"><i class="icon-play"></i></div>
                                    <div class="video-stop js-video-slider-stop"><i class="icon-pause"></i></div>
                                </div>
                            {% endif %}
                        </div>
                    {% else %}
                        {% if block.settings.image_mobile %}
                            <div class="bnslider-image-mobile lazyload fade-up-fast" data-bgset="{% include 'bgset', image: block.settings.image_mobile %}" data-sizes="auto"></div>
                            {% if section.settings.effect == 'flash' %}<div class="bnslider-image-mobile lazyload fade-up lightning flashit" data-bgset="{% include 'bgset', image: block.settings.image_mobile %}" data-sizes="auto" style="opacity: 0;"></div>{% endif %}
                        {% else %}
                            {% if block.settings.image %}
                                <div class="bnslider-image-mobile lazyload fade-up-fast" data-bgset="{% include 'bgset', image: block.settings.image %}" data-sizes="auto"></div>
                            {% else %}
                                <div class="bnslider-image-placeholder bnslider-image-mobile">
                                    {{ 'collection-5' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            {% endif %}
                        {% endif %}
                        {% if block.settings.image %}
                            <div class="bnslider-image lazyload fade-up-fast" data-bgset="{% include 'bgset', image: block.settings.image %}" data-sizes="auto"></div>
                            {% if section.settings.effect == 'flash' %}<div class="bnslider-image lazyload fade-up lightning flashit" data-bgset="{% include 'bgset', image: block.settings.image %}" data-sizes="auto" style="opacity: 0;"></div>{% endif %}
                        {% else %}
                            <div class="bnslider-image-placeholder bnslider-image">
                                {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
                            </div>
                        {% endif %}
                    {% endif %}
                    {% capture current %}{% cycle 1,2 %}{% endcapture %}
                    <div class="bnslider-text-wrap bnslider-overlay {% if section.settings.block_width == 'fullboxed' %}container{% endif %}">
                        {% if block.settings.bnr_decor_1 %}
                            <div class="bnr-decor" data-animation="{{ block.settings.bnr_decor_1_animation }}" data-animation-delay="{{ block.settings.bnr_decor_1_delay }}">
                                {% assign translateX = block.settings.bnr_decor_1_xpos %}
                                {% assign translateY = block.settings.bnr_decor_1_ypos %}
                                {% assign blend = block.settings.bnr_decor_1_blend %}
                                {% assign color = block.settings.bnr_decor_1_color %}
                                {% assign opacity = block.settings.bnr_decor_1_opacity | times: 0.01 %}
                                {% assign width = block.settings.bnr_decor_1_width %}
                                {% assign target = 'slide' %}
                                {%- render 'decor-circle-2', target: target, classes: class, opacity: opacity, rotate: rotate, blend: blend, color: color, opacity: opacity, translateX: translateX, translateY: translateY, width: width, blend: blend -%}
                            </div>
                        {% endif %}
                        {% if block.settings.bnr_decor_2 %}
                            <div class="bnr-decor" data-animation="{{ block.settings.bnr_decor_2_animation }}" data-animation-delay="{{ block.settings.bnr_decor_2_delay }}">
                                {% assign translateX = block.settings.bnr_decor_2_xpos %}
                                {% assign translateY = block.settings.bnr_decor_2_ypos %}
                                {% assign blend = block.settings.bnr_decor_2_blend %}
                                {% assign color = block.settings.bnr_decor_2_color %}
                                {% assign opacity = block.settings.bnr_decor_2_opacity | times: 0.01 %}
                                {% assign width = block.settings.bnr_decor_2_width %}
                                {% assign target = 'slide' %}
                                {%- render 'decor-circle-2', target: target, classes: class, opacity: opacity, rotate: rotate, blend: blend, color: color, opacity: opacity, translateX: translateX, translateY: translateY, width: width, blend: blend -%}
                            </div>
                        {% endif %}
                        <div class="bnslider-text-content {{ block.settings.captions_v_alignment }} {{ block.settings.captions_h_alignment }} {{ block.settings.captions_v_alignment_m }} {{ block.settings.captions_h_alignment_m }}">
                            <div class="bnslider-text-content-flex {% if section.settings.block_width == 'fullboxed' %} container {% endif %}">
                                <div class="bnslider-vert {% if block.settings.captions_width_enable %}w-s-{{ block.settings.captions_width }} w-ms-{{ block.settings.captions_width_m }}{% endif %}"
                                     style="padding: {{ block.settings.captions_padding }}px">
                                    {%- for i in (1..3) -%}
                                        {%- capture enable -%}caption_{{ i }}_yes{%- endcapture -%}
                                        {%- capture order -%}caption_{{ i }}_order{%- endcapture -%}
                                        {%- capture top -%}caption_{{ i }}_top{%- endcapture -%}
                                        {%- capture font -%}caption_{{ i }}_font{%- endcapture -%}
                                        {%- capture hidemobile -%}caption_{{ i }}_hidemobile_yes{%- endcapture -%}
                                        {%- capture text -%}caption_{{ i }}{%- endcapture -%}
                                        {%- capture animation -%}caption_{{ i }}_animation{%- endcapture -%}
                                        {%- capture delay -%}caption_{{ i }}_delay{%- endcapture -%}
                                        {%- capture color -%}caption_{{ i }}_color{%- endcapture -%}
                                        {%- capture bgcolor -%}caption_{{ i }}_bgcolor{%- endcapture -%}
                                        {%- capture fontweight -%}caption_{{ i }}_fontweight{%- endcapture -%}
                                        {%- capture fontsize -%}caption_{{ i }}_fontsize{%- endcapture -%}
                                        {%- capture lineheight -%}caption_{{ i }}_lineheight{%- endcapture -%}
                                        {%- capture othercss -%}caption_{{ i }}_othercss{%- endcapture -%}
                                        {%- if block.settings[enable] %}
                                            <div class="bnslider-text order-{{ block.settings[order] }} {{ block.settings[top] }} bnslider-text--{{ block.settings[fontsize] }}  text-{{ block.settings.text_h_alignment }} {% if block.settings[font] %}heading-font{% endif %} {% if block.settings[hidemobile] %} d-none d-md-block{% endif %} {%- unless block.settings[bgcolor] == '' or block.settings[bgcolor] contains 'rgba(0,0,0,0)' -%}bnslider-text--bg{% endunless %}" data-animation="{{ block.settings[animation] }}" data-animation-delay="{{ block.settings[delay] }}" data-fontcolor="{{ block.settings[color] }}" data-bgcolor="{{ block.settings[bgcolor] }}" data-fontweight="{{ block.settings[fontweight] }}" data-fontline="{{ block.settings[lineheight] }}" data-otherstyle="{{ block.settings[othercss] }}">{{ block.settings[text] }}</div>
                                        {%- endif -%}
                                    {% endfor %}
                                    {% if block.settings.link_button_yes %}
                                    <div class="btn-wrap text-{{ block.settings.text_h_alignment }}  order-{{ block.settings.link_button_order }} {{ block.settings.link_button_top }}"
                                         data-animation="{{ block.settings.button_animation }}"
                                         data-animation-delay="{{ block.settings.button_delay }}">
                                        {% unless block.settings.link_all_slide %}
                                        <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="btn {{ block.settings.button_size }}">
                                           {{ block.settings.link_button_text }}
                                        </a>
                                    {% else %}
                                        <div class="btn {{ block.settings.button_size }}">
                                            {{ block.settings.link_button_text }}
                                        </div>
                                        {% endunless %}
                                        </div>
                                    {% endif %}
                                    <div class="bnslider-vert-bg" style="{% if block.settings.captions_bg %}background: {{ block.settings.captions_bg }} {% endif %}; opacity: {{ block.settings.captions_bg_opacity | times: 0.01 }}; border-radius: {{ block.settings.captions_border_radius }}px"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    {% unless block.settings.link_all_slide %}
                </div>
            {% else %}
                </a>
                {% endunless %}
            {% endif %}
        {%- endfor %}
    </div>
    <div class="bnslider-loader">
    </div>
    <div class="bnslider-arrows {% unless section.settings.arrows %}{% if section.settings.dots %}d-sm-none{% else %}d-none{% endif %}{% endunless %} {% if section.settings.block_width == 'fullwidth' %}container-fluid{% else %}container{% endif %}">
        <div></div>
    </div>
    <div class="bnslider-dots {% unless section.settings.dots %}d-none{% else %}d-none d-sm-block{% endunless %} {% if section.settings.block_width == 'fullwidth' or section.settings.dotsfullwidth %}container-fluid{% else %}container{% endif %}"></div>
    <style>
        {%- for block in section.blocks -%}
        {%- if block.settings.link_button_yes -%}
        #shopify-section-{{ section.id }} [data-slick-index = "{{ forloop.index | minus:1 }}"]  .btn:not(.hovered):not(:hover) {
        {%- if block.settings.button_bg != blank and block.settings.button_bg != 'rgba(0,0,0,0)' -%}
            background-color: {{ block.settings.button_bg }};
        {%- endif -%} {%- if block.settings.button_color != blank and block.settings.button_color != 'rgba(0,0,0,0)' -%} color: {{ block.settings.button_color }};
        {%- endif -%}
        }
        #shopify-section-{{ section.id }} [data-slick-index = "{{ forloop.index | minus:1 }}"]  .btn:hover {
        {%- if block.settings.button_hover_bg != blank and block.settings.button_hover_bg != 'rgba(0,0,0,0)' -%}
            background-color: {{ block.settings.button_hover_bg }} !important;
        {%- endif -%} {%- if block.settings.button_hover_color != blank and block.settings.button_hover_color != 'rgba(0,0,0,0)' -%} color: {{ block.settings.button_hover_color }} !important;
        {%- endif -%}
        }
        {%- endif -%}
        {%- endfor -%}
    </style>
    {% if section.settings.snow %}
    {% if section.settings.slide_effect == 'sparkles' %}
    <div class="sparkles" id="SparklesEffect{{ section.id }}"></div>
    <style>
        .sparkle {
            position: absolute;
            bottom: 10px;
            left: 10px;
            display: block;
            height: 100px;
            width: 100px;
            background-image: url(data:image/png;base64,);
            background-repeat: no-repeat;
            background-position:center;
            background-size: 100% 100%;
            animation-name: sparkle;
            animation-duration: 4s;
            animation-timing-function: linear;
            animation-iteration-count: infinite;
            animation-fill-mode: forwards;
            opacity: 0;
        }
        @keyframes sparkle {
            0% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
            3% {
                transform: scale(1) rotate(15deg);
                opacity: 1;
            }
            6% {
                transform: scale(1) rotate(15deg);
                opacity: 1;
            }
            9% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
            100% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
        }

        @keyframes sparkleLeft {
            0% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
            3% {
                transform: scale(1) rotate(-15deg);
                opacity: 1;
            }
            6% {
                transform: scale(1) rotate(-15deg);
                opacity: 1;
            }
            9% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
            100% {
                transform: scale(0) rotate(0deg);
                opacity: 1;
            }
        }
    </style>
    <script>
		var sparkleTime = (function($) {
			var $container = $('#SparklesEffect{{ section.id }}'),
				$sparkle = $('<div class="sparkle"></div>'),
				totalSparkles,
				sparkleDelayRange = 15;

			if (window.matchMedia('(max-width: 730px)').matches) {
				totalSparkles = 40;
			} else {
				totalSparkles = 120;
			}

			function init() {
				sprinkleSparkles();
			}

			function sprinkleSparkles() {
				for (var i = 0; i < totalSparkles; i++ ) {
					var size = (i % 2 === 0) ? 'sparkle--small' : '';
					var direction = (i % 3 === 0) ? 'sparkle--left' : '';
					$sparkle
						.clone()
						.css({
							top: (Math.random() * 100) + '%',
							left: (Math.random() * 100) + '%',
							webkitAnimationDelay: (Math.random() * sparkleDelayRange) + 's',
							AnimationDelay: (Math.random() * sparkleDelayRange) + 's'
						})
						.addClass(size)
						.addClass(direction)
						.appendTo($container);
				}
			}

			return {
				init: init
			};
		})(jQuery);

		sparkleTime.init();
    </script>
    {% elsif section.settings.slide_effect == 'snow' %}
        <canvas class="snow-effect" id="SnowEffect{{ section.id }}"></canvas>
        <style>
            .snow-effect {
                position: absolute;
                overflow: hidden;
                top: 0;
                pointer-events: none;
            }
        </style>
        <script>
			initLetItSnow('.snow-effect', 50, 150);
			function initLetItSnow(el, mCount, dCount) {
				var requestAnimationFrame = window.requestAnimationFrame || window.mozRequestAnimationFrame || window.webkitRequestAnimationFrame || window.msRequestAnimationFrame || function (callback) {
						window.setTimeout(callback, 1000 / 60);
					};
				window.requestAnimationFrame = requestAnimationFrame;
				var flakes = [],
					canvas = $(el)[0],
					ctx = canvas.getContext("2d"),
					mX = -100,
					mY = -100;
				if ($(window).width() < 1025) {
					var flakeCount = mCount;
				} else {
					var flakeCount = dCount;
				}
				canvas.width = window.innerWidth;
				canvas.height = window.innerHeight;
				function snow() {
					ctx.clearRect(0, 0, canvas.width, canvas.height);
					for (var i = 0; i < flakeCount; i++) {
						var flake = flakes[i],
							x = mX,
							y = mY,
							minDist = 250,
							x2 = flake.x,
							y2 = flake.y;
						var dist = Math.sqrt((x2 - x) * (x2 - x) + (y2 - y) * (y2 - y)),
							dx = x2 - x,
							dy = y2 - y;
						if (dist < minDist) {
							var force = minDist / (dist * dist),
								xcomp = (x - x2) / dist,
								ycomp = (y - y2) / dist,
								// deltaV = force / 2;
								deltaV = force;
							flake.velX -= deltaV * xcomp;
							flake.velY -= deltaV * ycomp;
						} else {
							flake.velX *= .98;
							if (flake.velY <= flake.speed) {
								flake.velY = flake.speed;
							}
							flake.velX += Math.cos(flake.step += .05) * flake.stepSize;
						}
						ctx.fillStyle = "rgba(255,255,255," + flake.opacity + ")";
						flake.y += flake.velY;
						flake.x += flake.velX;
						if (flake.y >= canvas.height || flake.y <= 0) {
							reset(flake);
						}
						if (flake.x >= canvas.width || flake.x <= 0) {
							reset(flake);
						}
						ctx.beginPath();
						ctx.arc(flake.x, flake.y, flake.size, 0, Math.PI * 2);
						ctx.fill();
					}
					requestAnimationFrame(snow);
				};
				function reset(flake) {
					flake.x = Math.floor(Math.random() * canvas.width);
					flake.y = 0;
					flake.size = Math.random() * 3 + 5;
					flake.speed = Math.random() * 1 + 0.5;
					flake.velY = flake.speed;
					flake.velX = 0;
					flake.opacity = Math.random() * 0.5 + 0.3;
				}

				function init() {
					for (var i = 0; i < flakeCount; i++) {
						var x = Math.floor(Math.random() * canvas.width),
							y = Math.floor(Math.random() * canvas.height),
							size = Math.random() * 3 + 4,
							speed = Math.random() * 1 + 0.5,
							opacity = Math.random() * 0.5 + 0.3;
						flakes.push({
							speed: speed,
							velY: speed,
							velX: 0,
							x: x,
							y: y,
							size: size,
							stepSize: Math.random() / 160,
							step: 0,
							opacity: opacity
						});
					}
					snow();
				};
				$(window).on("resize", function () {
					canvas.width = window.innerWidth;
					canvas.height = window.innerHeight;
				})
				init();
			}
        </script>
    {% endif %}
    {% endif %}
    </div>
    </div>
    </div>
{% endif %}
<style>
    .lightning{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        filter: brightness(3);
        z-index: 1;
        opacity: 0;
    }
    .flashit{
        animation: flash ease-out 5s infinite;
        animation-delay: 0s;
    }

    @-webkit-keyframes flash {
        from { opacity: 0; }
        92% { opacity: 0; }
        93% { opacity: 0.6; }
        94% { opacity: 0.2; }
        96% { opacity: 0.9; }
        to { opacity: 0; }
    }

    @keyframes flash {
        from { opacity: 0; }
        92% { opacity: 0; }
        93% { opacity: 0.6; }
        94% { opacity: 0.2; }
        96% { opacity: 1; }
        to { opacity: 0; }
    }
</style>
