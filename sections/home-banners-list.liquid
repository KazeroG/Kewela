<div class="holder {{ section.settings.outer_margin }} {% if section.settings.enable_bg %}holder-with-bg {{ section.settings.padding-t }} {{ section.settings.padding-b }}  bg-cover lazyload" data-expand="-500"
     {%- if section.settings.bg_image != blank -%}data-bgset="{% include 'bgset', image: section.settings.bg_image %}" data-sizes="auto"{%- endif -%}
     {%- if section.settings.bg_color != blank and section.settings.bg_color != 'rgba(0,0,0,0)' -%}style="background-color: {{ section.settings.bg_color }}"{% endif %}
        {%- else -%}"{%- endif -%}>
<div class="{% if section.settings.block_width == "boxed" %}container{% else %}container-fluid px-0{% endif %}">
    {% include 'section-headings' %}
    <div class="row grid vert-margin-small">
        {%- for block in section.blocks -%}
            {%- case block.type -%}
            {%- when 'banner' -%}
            {%- if block.settings.enable -%}
                <div {{ block.shopify_attributes }} class="col-{{ block.settings.col }} col-md-{{ block.settings.col_md }}">
                    {%- assign banner = block.settings -%}
                    {%- render 'banner-item', banner: banner, id: block.id, id: block.id, gutters: gutters -%}
                </div>
            {%- endif -%}
            {%- when 'list' -%}
            <div {{ block.shopify_attributes }} id="block-{{ block.id }}" class="col-{{ block.settings.col }} col-md-{{ block.settings.col_md }} col-bnr-categories-list">
                <div class="fade-up bg-cover w-100 h-100 lazyload"
                     {%- if block.settings.bg_image != blank -%}data-bgset="{% include 'bgset', image: block.settings.bg_image %}" data-sizes="auto"{%- endif -%}
                        {%- if block.settings.bg_color != blank and block.settings.bg_color != 'rgba(0,0,0,0)' -%}style="background-color: {{ block.settings.bg_color }}"{% endif %}>
                    <div class="bnr-categories {% if block.settings.font %}heading-font{% endif %}">
                        {% if block.settings.title != blank %}
                            <h3 class="bnr-categories-title">{{ block.settings.title }}</h3>
                        {%- endif -%}
                        <div class="row flex-md-nowrap">
                            {% if block.settings.text != blank %}
                            <div class="col-md-auto">
                                <ul class="bnr-categories-list">
                                    {% for link in linklists[block.settings.text].links %}
                                    <li><a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a></li>
                                    {% endfor %}
                                </ul>
                            </div>
                            {%- endif -%}
                            {% if block.settings.text1 != blank %}
                            <div class="col-md-auto">
                                <ul class="bnr-categories-list">
                                    {% for link in linklists[block.settings.text1].links %}
                                        <li><a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a></li>
                                    {% endfor %}
                                </ul>
                            </div>
                            {%- endif -%}
                        </div>
                    </div>
                </div>
            </div>
            {%- when 'text' -%}
            <div {{ block.shopify_attributes }} id="block-{{ block.id }}" class="col-{{ block.settings.col }} col-md-{{ block.settings.col_md }} col-bnr-categories-list">
                <div class="fade-up bg-cover w-100 h-100 lazyload"
                     {%- if block.settings.bg_image != blank -%}data-bgset="{% include 'bgset', image: block.settings.bg_image %}" data-sizes="auto"{%- endif -%}
                        {%- if block.settings.bg_color != blank and block.settings.bg_color != 'rgba(0,0,0,0)' -%}style="background-color: {{ block.settings.bg_color }}"{% endif %}>
                    <div class="bnr-categories">
                        {% if block.settings.title != blank %}
                            <h3 class="bnr-categories-title">{{ block.settings.title }}</h3>
                        {%- endif -%}
                        <div class="row flex-md-nowrap">
                            {% if block.settings.text != blank %}
                                <div class="col-md-auto">
                                    {{ block.settings.text }}
                                </div>
                            {%- endif -%}
                            {% if block.settings.text1 != blank %}
                                <div class="col-md-auto">
                                   {{ block.settings.text1 }}
                                </div>
                            {%- endif -%}
                        </div>
                    </div>
                </div>
            </div>
            {%- endcase -%}
        {%- endfor -%}
    </div>
</div>
</div>
<style>
    .bnr-categories-list li a {
        transition: none;
    }
    {%- for block in section.blocks -%}
    #block-{{ block.id }} .bnr-categories-list li {
        font-size: {{ block.settings.text_size }}px;
    }
    {% if block.settings.color != blank and block.settings.color != 'rgba(0,0,0,0)' %}
        #block-{{ block.id }} .bnr-categories, #block-{{ block.id }} .bnr-categories-title {
            color: {{ block.settings.color }};
        }
    {%- endif -%}
    {% if block.settings.link_color != blank and block.settings.link_color != 'rgba(0,0,0,0)' %}
    #block-{{ block.id }} .bnr-categories a {
        color: {{ block.settings.link_color }};
    }
    {%- endif -%}
    {% if block.settings.link_bg != blank and block.settings.link_bg != 'rgba(0,0,0,0)' %}
    #block-{{ block.id }} .bnr-categories-list li {
        background: {{ block.settings.link_bg }};
        padding-left: 20px;
        padding-right: 20px;
    }
    {%- endif -%}
    {% if block.settings.link_hover_color != blank and block.settings.link_hover_color != 'rgba(0,0,0,0)' %}
    #block-{{ block.id }} .bnr-categories-list li {
        padding-left: 20px;
        padding-right: 20px;
    }
    #block-{{ block.id }} .bnr-categories a:hover {
        color: {{ block.settings.link_hover_color }};
    }
    {%- endif -%}
    {% if block.settings.link_hover_bg != blank and block.settings.link_hover_bg != 'rgba(0,0,0,0)' %}
    #block-{{ block.id }} .bnr-categories-list li:hover {
        background: {{ block.settings.link_hover_bg }};
    }
    {% if block.settings.link_hover_color != blank and block.settings.link_hover_color != 'rgba(0,0,0,0)' %}
    #block-{{ block.id }} .bnr-categories-list li:hover a {
        color: {{ block.settings.link_hover_color }};
    }
    {%- endif -%}
    {%- endif -%}
    {% if block.settings.list_style == 'nomarker' %}
    .bnr-categories-list li:before {
        display: none;
    }
    {%- endif -%}
    {%- endfor -%}
</style>
