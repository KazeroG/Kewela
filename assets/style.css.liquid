{
    .circle-loader-wrap .circle-loader-pagination
    {
        display: none;
    }
}
.hide-showmore .circle-loader-wrap
{
    display: none !important;
}
.circle-loader
{
    position: relative;
    cursor: pointer;
}
.circle-loader-text,
.circle-loader-text-alt
{
    font-size: 13px;
    font-weight: 600;
    line-height: 1em;
    position: absolute;
    top: 0;
    left: 0;
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: 100%;
    text-transform: uppercase; 
    -ms-flex-align: center;
    align-items: center;
    -ms-flex-pack: center;
    justify-content: center;
}
