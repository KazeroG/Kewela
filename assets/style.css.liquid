    max-width: 1200px;
}
body.boxed .page-wrapper
{
    overflow: hidden;
    max-width: {{ container-boxed }};
    margin-right: auto;
    margin-left: auto;
    background-color: {{ body_bg }};
    box-shadow: 0 0 50px rgba(0, 0, 0, .1);
}
@supports (-webkit-overflow-scrolling: touch)
{
    .row:before,
    .custom-grid:before
    {
        content: none;
    }
    .row:after,
    .custom-grid:after
    {
        content: '';
    }
}
body.mac .row:before,
body.mac .row:after,
body.mac .custom-grid:before,
body.mac .custom-grid:after
{
    display: none;
}
