{
    .circle-loader-text-alt
    {
        display: none;
    }
    .circle-loader:hover .circle-loader-text
    {
        display: none;
    }
    .circle-loader:hover .circle-loader-text-alt
    {
        display: -ms-flexbox;
        display: flex;
    }
    .circle-loader > a
    {
